  <template name="http" value="var params = {&#10;    &quot;pageIndex&quot;: pageIndex,&#10;    &quot;pageSize&quot;: pageSize,&#10;};&#10;_onSuccessCallback(data) {&#10;  &#10;}&#10;_onFailureCallback(code, errorMsg) {&#10;    showError(code,errorMsg);&#10;}&#10;asyncRequest(() =&gt; TioRepositories.to.$API$(params), onSuccess: _onSuccessCallback, onFailure: _onFailureCallback);&#10;$END$" description="http请求模板" toReformat="false" toShortenFQNames="true">
