    </context>
  </template>
  <template name="timxe" value="timber.log.Timber.tag(&quot;DONG&quot;).e(e,&quot;$METHOD_NAME$: $content$&quot;)" description="Timber xxx Exception" toReformat="true" toShortenFQNames="true">
